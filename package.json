{
  "name": "gg-deploy",
  "version": "2.5.0",
  "description": "Free hosting deserves free tooling. Domain â†’ GitHub Pages in 60 seconds.",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "gg-deploy": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli.ts",
    "start": "node dist/cli.js",
    "lint": "eslint src --ext .ts",
    "test": "vitest",
    "prepublishOnly": "npm run build",
    "build:binary": "node scripts/build-binary.mjs",
    "bundle": "esbuild dist/cli.js --bundle --platform=node --target=node18 --outfile=bin/cli.bundle.cjs --format=cjs --external:fsevents --minify",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build"
  },
  "keywords": [
    "cli",
    "github-pages",
    "deployment",
    "dns",
    "godaddy",
    "cloudflare",
    "namecheap",
    "git-free",
    "no-git",
    "static-site",
    "hosting",
    "automation",
    "mcp",
    "ai-native",
    "claude",
    "cursor",
    "windsurf",
    "model-context-protocol"
  ],
  "author": "Abe Diaz",
  "license": "AGPL-3.0-only",
  "homepage": "https://abe238.github.io/gg-deploy/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abe238/gg-deploy.git"
  },
  "bugs": {
    "url": "https://github.com/abe238/gg-deploy/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.1",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "ora": "^8.1.1"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.9.6",
    "@types/node": "^22.10.2",
    "esbuild": "^0.24.2",
    "eslint": "^9.17.0",
    "pkg": "^5.8.1",
    "puppeteer": "^24.34.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "pkg": {
    "scripts": "bin/cli.bundle.cjs",
    "assets": [
      "ui/dist/**/*"
    ],
    "targets": [
      "node18-macos-arm64",
      "node18-macos-x64",
      "node18-linux-x64",
      "node18-linux-arm64",
      "node18-win-x64"
    ],
    "outputPath": "bin"
  }
}
